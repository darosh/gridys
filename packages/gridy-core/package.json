{
  "name": "@gridy/core",
  "description": "TypeScript grid library for server and browser",
  "version": "0.4.6",
  "author": "Jan Forst <ensonador@gmail.com>",
  "devDependencies": {
    "@gridy/build-lib": "^1.0.0",
    "@gridy/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "d3": "5.9.2",
    "del-cli": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "jest": "^24.8.0",
    "jsdom": "15.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "license": "MIT",
  "main": "dist/gridy-core.umd.js",
  "meta": {
    "namespace": "Gridy"
  },
  "module": "dist/gridy-core.es.js",
  "repository": ".",
  "scripts": {
    "build": "tsc --module commonjs && node node_modules/@gridy/build-lib/node_modules/rollup/bin/rollup -c node_modules/@gridy/build-lib/rollup.config.js && node node_modules/@gridy/build-lib/node_modules/typedoc/bin/typedoc --out docs --target es6 --theme minimal --mode file src",
    "generate:examples": "node examples/node.js",
    "lint": "eslint --fix src/**/*.ts",
    "prebuild": "del-cli dist docs",
    "prepublish": "npm run build",
    "test": "jest --coverage"
  },
  "types": "dist/types/index.d.ts",
  "typpings": "dist/types/index.d.ts"
}
