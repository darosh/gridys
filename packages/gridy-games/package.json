  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "private": true,
  "scripts": {
    "build": "webpack --mode production -p",
    "build:dev": "cross-env USE_FIREBASE=1 webpack --mode development",
    "build:prod": "cross-env cross-env USE_GA=UA-117246854-1 USE_CNAME=1 npm run build",
    "build:report": "npm run build --report",
    "build:stats": "webpack --profile --json > stats.json",
    "cert": "openssl req -x509 -nodes -days 365 -newkey rsa:2048 -keyout key.pem -out cert.pem",
    "clean": "del-cli dist/*.js dist/*.html dist/static dist/workbox* dist/CNAME",
    "deploy": "npm run clean && npm --no-git-tag-version version patch && cross-env USE_GA=UA-117246854-1 USE_CNAME=1 npm run build && deploy-to-gh-pages --local ./dist",
    "icons": "real-favicon generate icon.config.json .icons/data.json .icons",
    "install": "node ./plugins/heroes.bundle.js > ./plugins/heroes.json && babel-external-helpers > plugins/helpers.js && rollup -c rollup.config.js -f es -o plugins/icons.js plugins/icons.bundle.js && cross-env NODE_ENV=es5 babel build/vuetify/src --out-dir build/vuetify/es5 && node ./build/patch.js vuetify",
    "lint": "eslint -f unix --fix --ext .js,.vue src build firebase",
    "serve": "hs ./dist -p 443 -S",
    "start": "cross-env USE_FIREBASE=1 USE_GA=UA-117246854-1 DEBUG_GA=\"false\" NODE_ENV=development webpack-dev-server --inline --hot --open --host 0.0.0.0 --useLocalIp",
    "test": "jest --coverage",
    "watch": "concurrently \"cd ../gridy && npm start\" \"cd ../gridy-games-lib && npm start\""
  },
  "storageVersion": "0.1.144"
}
