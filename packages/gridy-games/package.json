{
  "name": "@gridy/games",
  "description": "Board games library",
  "version": "0.1.2",
  "author": "Jan Forst <ensonador@gmail.com>",
  "dependencies": {
    "@gridy/core": "^0.4.6"
  },
  "devDependencies": {
    "@gridy/build": "^1.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2"
  },
  "license": "MIT",
  "main": "dist/gridy-games.umd.js",
  "meta": {
    "namespace": "GridyGames",
    "external": [
      "@gridy/core"
    ]
  },
  "module": "dist/gridy-games.es.js",
  "scripts": {
    "build": "tsc --module commonjs && rollup -c node_modules/@gridy/build/rollup.config.js",
    "doc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "prebuild": "del-cli dist",
    "predoc": "del-cli docs",
    "prepublish": "npm run build",
    "start": "rollup -c node_modules/@gridy/build/rollup.config.js -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage"
  },
  "typings": "dist/types/index.d.ts"
}
